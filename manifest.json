{
  "name": "Lefty",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Move Page Left",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "background": {
    "scripts": [
      "jquery-2.0.2.min.js",
      "schema.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16x16.png",
    "l9": "iconl9xl9.png",
    "32": "icon32x32.png",
    "48": "icon48x48.png",
    "128": "icon128x128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon19x19.png",
    "default_title": "Lefty"
  }
}
